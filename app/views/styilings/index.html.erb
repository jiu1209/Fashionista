<div class="indextot">
  <div class= "container">
	<div class="row">	
		<div class="col-xs-3">

			 <%= render 'layouts/styling' %> 

		</div>
	      <div class="col-xs-9">
	      	<h2> Styling All</h2>
	      	  <%  @styilings.each do |styiling| %>
               <div class="userbox">
                <table>
              	  <tr>
                    <td><%=  attachment_image_tag styiling, :styling_image, format: "jpeg", size: "200x250" %></td>
                  </tr>
                  <tr>
                    <td><%= link_to styiling.item_one, styiling_path(styiling.id) %> | <%= link_to styiling.brand_one, styiling_path(styiling.id) %></td>
                  </tr>
                </table>
              </div>
           <% end %>
          <div align="right"><%= paginate @styilings %></div>      
