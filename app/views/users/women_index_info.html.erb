<div class="indextot">
  <div class= "container">
	  <div class="row">	
		  <div class="col-xs-3">

			 <%= render 'layouts/styling_info' %> 

		  </div>
	    <div class="col-xs-9">
	      <h2>Women's</h2>
	      	<% @users.each do |user| %>
            <div class="userbox">
              <table>
              	<tr>
                  <td><%=link_to attachment_image_tag(user, :image, format: "jpeg", fallback:"no_image.jpg", size: "200x200"), show_info_path(user.id) %></td>
                </tr>
                <tr>
                  <td><%= link_to user.name, show_info_path(user.id) %>　　
                </tr>
                <tr>
                  <td><%= user.height %> | <%= user.gender %> | <%= user.favorite_brand %></td>
                </tr>
              </table>
            </div>
          <% end %>
          <div class='p-eu__paginate'>
            <div align="right"><%= paginate @users %></div>
          </div>
       </div>
    </div>
  </div>
</div>